---
layout: layouts/base.njk
templateClass: tmpl-transparency
---

<center>
    <h1>APIs</h1>
</center>


<div style="text-align: center;">
    <div style="display: inline-block; text-align: left;">
        {{ content | safe }}
    </div>
</div>

<center>
    <h3>{{duckOpenApi.info.title}}</h3>
    <ul id="duck-list" style="list-style: none;">
        <li>
            <label style="color:green;">GET</label>
            {{duckOpenApi.servers[0].url}}/random</li>
        <li>
            <b>{{duckOpenApi.paths["/random"].get.description}}</b>
        </li>
        <li>
            <span>
                <label style="color:green;">
                    <b>RESPONSE:</b>
                </label>
                <mark>
                    <code>{{duckOpenApi.paths["/random"].get.responses["200"].example | dump}}</code>
                </mark>
            </span>
        </li>
    </ul>
    <h3>{{dogOpenApi.info.title}}</h3>
    <ul id="dog-list" style="list-style: none;">
        <li>
            <label style="color:green;">GET</label>
            {{dogOpenApi.servers[0].url}}/breeds/image/random</li>
        <li>
            <b>{{dogOpenApi.paths["/breeds/image/random"].get.description}}</b>
        </li>
        <li>
            <span>
                <label style="color:green;">
                    <b>RESPONSE:</b>
                </label>
                <mark>
                    <code>{{dogOpenApi.paths["/breeds/image/random"].get.responses["200"].example | dump}}</code>
                </mark>
            </span>
        </li>
    </ul>

    <h3>{{analytesOpenApi.info.title}}</h3>
    <ul id="analytes-list" style="list-style: none;">
        <li>
            <label style="color:green;">GET</label>
            {{analytesOpenApi.servers[0].url}}/analytes</li>
        <li>
            <b>{{analytesOpenApi.paths["/analytes"].get.description}}</b>
        </li>
        <li>
            <span>
                <label style="color:green;">
                    <b>RESPONSE:</b>
                </label>
                <mark>
                    <code>{{analytesOpenApi.paths["/analytes"].get.responses["200"].example | dump}}</code>
                </mark>
            </span>
        </li>
    </ul>
</center>